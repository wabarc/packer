{
  "name": "@wabarc/packer",
  "version": "0.1.0",
  "description": "Pack and store webpages from Telegram channel.",
  "main": "./dist/index.js",
  "types": "./dist/type.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format:check": "prettier --check '**/*.ts'",
    "lint": "eslint ./ --ext .ts",
    "test": "jest --testTimeout 300000",
    "clean": "rm -rf ./dist; rm -rf *.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wabarc/packer.git"
  },
  "keywords": [
    "archive",
    "wayback",
    "telegram"
  ],
  "author": "Wayback Archiver",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/wabarc/packer/issues"
  },
  "homepage": "https://github.com/wabarc/packer#readme",
  "dependencies": {
    "@wabarc/archiver": "^0.2.0",
    "html-minifier": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7.11.0",
    "eslint-plugin-jest": "^24.1.0",
    "jest": "^26.5.3",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3",
    "typescript-formatter": "^7.2.2"
  },
  "engines": {
    "node": ">= 12"
  }
}
